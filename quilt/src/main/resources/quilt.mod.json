{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${group}",
    "id": "vrapi",
    "version": "${version}",
    "name": "mc-vr-api",
    "description": "VR API mod that allows mods to integrate into Vivecraft.",
    "authors": ["hammy3502"],
    "contact": {
      "sources": "https://github.com/hammy3502/mc-vr-api"
    },
    "license": "GNU GPLv3",
    "icon": "assets/examplemod/icon.png",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "init": [
        "net.blf02.quilt.VRAPIQuilt"
      ],
      "vrapi": [
        "net.blf02.vrapi.common.VRAPI"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.19.3"
      },
      {
        "id": "architectury",
        "version": ">=7.0.66"
      }
    ]
  }
}